{
  "name": "andersen-blocks",
  "version": "1.0.0",
  "description": "Custom blocks for Gutenberg",
  "scripts": {
    "build": "npm build:latest-projects && npm build:project-info",
    "build:project-info": "wp-scripts build project-info/src/index.js --output-path=project-info/build",
    "build:latest-projects": "wp-scripts build latest-projects/src/index.js --output-path=latest-projects/build"
  },
  "author": "Astrid Mathilde Andersen",
  "license": "UNLICENSED",
  "devDependencies": {
    "@wordpress/components": "^9.2.0",
    "@wordpress/scripts": "^7.0.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
}
